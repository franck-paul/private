<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:wfw="http://wellformedweb.org/CommentAPI/" xml:lang="{{tpl:BlogLanguage}}">
  <title type="html">{{tpl:lang Restricted area}}</title>
  <link href="{{tpl:SysSelfURI}}" rel="self" type="application/atom+xml" />
  <link href="{{tpl:BlogURL}}" rel="alternate" type="text/html" title="{{tpl:lang Restricted area}}" />
  <updated>{{tpl:BlogUpdateDate iso8601="1"}}</updated>
  <author>
    <name>{{tpl:BlogEditor encode_xml="1"}}</name>
  </author>
  <id>{{tpl:BlogFeedID}}</id>
  <generator uri="http://www.dotclear.org/">Dotclear</generator>
  <entry>
    <title>{{tpl:lang Restricted area}}</title>
    <link href="{{tpl:BlogURL}}" rel="alternate" type="text/html" title="{{tpl:lang Restricted area}}" />
    <id>{{tpl:BlogFeedID}}</id>
    <updated>{{tpl:BlogUpdateDate iso8601="1"}}</updated>
    <author>
      <name>{{tpl:BlogEditor encode_xml="1"}}</name>
    </author>
    <content type="html">{{tpl:PrivateMsg absolute_urls="1" encode_xml="1"}}</content>
  </entry>
</feed>
